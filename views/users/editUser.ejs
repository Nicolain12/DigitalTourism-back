<!DOCTYPE html>
<html lang="en">

<%- include('../partials/head') %>

    <body>

        <%- include('../partials/header') %>

            <main>
                <form action="" enctype="multipart/form-data" method="POST" class="main-profile-div-EDIT">
                    <section class="img-profile-EDIT">
                        <label for="fileEdit">
                            <img src="/images/users/default.jpg" alt="Imagen del usuario">
                            <p>Editar foto</p>
                        </label>
                        <input type="file" value="" name="fileEdit" id="fileEdit">
                    </section>
                    <section class="main-info-profile-EDIT">
                        <div class="name-edit">
                            <label for="">Nombre:</label>
                            <input type="text" value="" name="nameEdit" id="nameEdit">
                        </div>
                        <div class="surname-edit">
                            <label for="surnameEdit">Apellido:</label>
                            <input type="text" value="" name="surnameEdit" id="surnameEdit">
                        </div>
                        <div class="email-edit">
                            <label for="emailEdit">Email:</label>
                            <input type="email" value="" name="emailEdit" id="emailEdit">
                        </div>
                        <div class="button-profile-edit">
                            <button type="submit">Editar</button>
                        </div>
                    </section>


                </form>
            </main>

            <%- include('../partials/footer') %>

    </body>

</html>